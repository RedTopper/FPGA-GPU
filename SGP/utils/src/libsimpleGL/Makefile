	/usr/bin/cc -DHAVE_X11 -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/build/thirdparty/libbacktrace -Isrc/thirdparty/libbacktrace -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include -Isrc/thirdparty/zlib -Isrc/thirdparty/libbacktrace/auxincl  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -funwind-tables -Wno-switch -Wno-enum-compare -o build/simple.c.o   -c src/thirdparty/libbacktrace/simple.c
