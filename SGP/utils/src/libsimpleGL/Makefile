	/usr/bin/c++ -DGTEST_HAS_TR1_TUPLE=0 -DHAVE_BACKTRACE=1 -DHAVE_X11 -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Dglxtrace_EXPORTS  -I$(INCLUDE_PATH) -Isrc/wrappers -Isrc/thirdparty/gtest/include -Isrc/thirdparty/khronos -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include -Isrc/thirdparty/zlib -Isrc/thirdparty/libbacktrace -Isrc/build/thirdparty/libbacktrace -Isrc/lib/trace -Isrc/lib/os -Isrc/helpers -Isrc/build/dispatch -Isrc/dispatch -Isrc/lib/guids -Isrc/thirdparty/crc32c  -fvisibility=hidden -std=gnu++11 -Wall -Wno-sign-compare -fno-strict-aliasing -fno-rtti -fno-exceptions -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC  -o build/glxtrace.cpp.o -c src/glxtrace.cpp  

build/dlsym.cpp.o: src/wrappers/dlsym.cpp 	
	/usr/bin/c++ -DGTEST_HAS_TR1_TUPLE=0 -DHAVE_BACKTRACE=1 -DHAVE_X11 -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Dglxtrace_EXPORTS  -I$(INCLUDE_PATH) -Isrc/wrappers -Isrc/thirdparty/gtest/include -Isrc/thirdparty/khronos -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include -Isrc/thirdparty/zlib -Isrc/thirdparty/libbacktrace -Isrc/build/thirdparty/libbacktrace -Isrc/lib/trace -Isrc/lib/os -Isrc/helpers -Isrc/build/dispatch -Isrc/dispatch -Isrc/lib/guids -Isrc/thirdparty/crc32c  -fvisibility=hidden -std=gnu++11 -Wall -Wno-sign-compare -fno-strict-aliasing -fno-rtti -fno-exceptions -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC  -o build/dlsym.cpp.o -c src/wrappers/dlsym.cpp   


glxtrace.so: ../../lib64/libSGP.a build/dlsym.cpp.o build/glxtrace.cpp.o build/libgltrace_common.a build/libglhelpers.a build/libglproc_gl.a build/libglproc.a build/libtrace.a build/libcrc32c.a build/libsnappy_bundled.a build/libcommon.a build/libhighlight.a build/libos.a build/libbacktrace.a build/libbrotli_dec.a build/libbrotli_common.a build/libguids.a
	/usr/bin/c++ -fPIC  -fvisibility=hidden -std=gnu++11 -Wall -Wno-sign-compare -fno-strict-aliasing -fno-rtti -fno-exceptions -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG  -Wl,--version-script=src/wrappers/glxtrace.version   -Wl,-Bsymbolic -Wl,-Bsymbolic-functions -Wl,-z,defs -shared -Wl,-soname,glxtrace.so -o ../../lib64/glxtrace.so build/glxtrace.cpp.o build/dlsym.cpp.o build/libgltrace_common.a build/libglproc_gl.a -lpthread -ldl build/libglhelpers.a build/libtrace.a build/libcommon.a build/libhighlight.a build/libos.a build/libbacktrace.a build/libbrotli_dec.a build/libbrotli_common.a build/libguids.a build/libcrc32c.a build/libsnappy_bundled.a build/libglproc.a $(LDFLAGS) -ldl 



../../lib64/libSGP.a:
	make -C ../libSGP/ all

makedirectories:
	mkdir -p build

clean:
	rm -f *~ *.o *.so
	rm -f ../../lib64/libGL*
	rm -f ../../lib64/glxtrace*
	rm -rf build
