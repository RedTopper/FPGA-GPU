###############################################################################
# Makefile for creating apitrace GL wrapper library. This is all hand-coded 
# straight from cmake, so no judging. 
###############################################################################

CXX=g++
CFLAGS+=
INCLUDE_PATH+=../../include/
LDFLAGS = -L../../lib64 -lshaderc_shared -llua -lSGP 
all: makedirectories libGL.so.1.2


libGL.so: glxtrace.so 
	ln -sf $< ../../lib64/$@


libGL.so.1: libGL.so 
	ln -sf $< ../../lib64/$@

libGL.so.1.2: libGL.so.1
	ln -sf $< ../../lib64/$@




build/libguids.a:
	/usr/bin/c++  -DGTEST_HAS_TR1_TUPLE=0 -DHAVE_BACKTRACE=1 -DHAVE_X11 -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/thirdparty/gtest/include -Isrc/thirdparty/khronos -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include -Isrc/thirdparty/zlib -Isrc/thirdparty/libbacktrace -Isrc/build/thirdparty/libbacktrace -Isrc/lib/trace -Isrc/lib/os  -fvisibility=hidden -std=gnu++11 -Wall -Wno-sign-compare -fno-strict-aliasing -fno-rtti -fno-exceptions -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG  -o build/guids.cpp.o -c src/lib/guids/guids.cpp
	/usr/bin/ar qc build/libguids.a  build/guids.cpp.o
	/usr/bin/ranlib build/libguids.a


build/libbrotli_common.a:
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/dictionary.c.o   -c src/thirdparty/brotli/c/common/dictionary.c
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/transform.c.o   -c src/thirdparty/brotli/c/common/transform.c
	/usr/bin/ar qc build/libbrotli_common.a  build/dictionary.c.o build/transform.c.o
	/usr/bin/ranlib build/libbrotli_common.a

build/libbrotli_dec.a:
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/bit_reader.c.o   -c src/thirdparty/brotli/c/dec/bit_reader.c
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/decode.c.o   -c src/thirdparty/brotli/c/dec/decode.c
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/huffman.c.o   -c src/thirdparty/brotli/c/dec/huffman.c
	/usr/bin/cc -DHAVE_X11 -DOS_LINUX -D_GNU_SOURCE -D__STDC_FORMAT_MACROS -D__STDC_LIMIT_MACROS -Isrc/compat -Isrc/thirdparty/snappy -Isrc/thirdparty/brotli/c/include  -fvisibility=hidden -Wall -Wno-sign-compare -fno-strict-aliasing -fmessage-length=0 -fno-omit-frame-pointer -O2 -g -DNDEBUG -fPIC   -o build/state.c.o   -c src/thirdparty/brotli/c/dec/state.c
	/usr/bin/ar qc build/libbrotli_dec.a  build/bit_reader.c.o build/decode.c.o build/huffman.c.o build/state.c.o
	/usr/bin/ranlib build/libbrotli_dec.a


build/libbacktrace.a:
